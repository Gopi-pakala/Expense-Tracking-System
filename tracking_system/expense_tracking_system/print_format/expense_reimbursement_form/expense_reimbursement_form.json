 "html": "<h2 style=\"text-align:center;\">Expense Reimbursement Form</h2>\r\n<hr><br>\r\n\r\n<table style=\"width:100%; font-size: 13px;\">\r\n    <tr>\r\n        <td style=\"width:35%;\"><b>Employee:</b></td>\r\n        <td>{{ doc.employee }}</td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td><b>Expense Category:</b></td>\r\n        <td>{{ doc.expense_category }}</td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td><b>Posting Date:</b></td>\r\n        <td>{{ doc.posting_date }}</td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td><b>Vendor:</b></td>\r\n        <td>{{ doc.vendor }}</td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td><b>Amount:</b></td>\r\n        <td>\u20b9{{ doc.amount }}</td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td><b>Description:</b></td>\r\n        <td>{{ doc.description }}</td>\r\n    </tr>\r\n\r\n    {% if doc.attachment %}\r\n    <tr>\r\n        <td><b>Attachment:</b></td>\r\n        <td>Attached ({{ doc.attachment }})</td>\r\n    </tr>\r\n    {% endif %}\r\n</table>\r\n\r\n<br><br>\r\n<hr>\r\n\r\n<h4>Declarations</h4>\r\n<ul>\r\n    <li>I hereby confirm that the above expense was incurred for official purposes.</li>\r\n    <li>I request reimbursement for the amount mentioned.</li>\r\n</ul>\r\n\r\n<br><br>\r\n\r\n<table style=\"width:100%; margin-top:20px;\">\r\n    <tr>\r\n        <td style=\"width:50%;\">\r\n            <b>Employee Signature:</b><br><br><br>\r\n            ____________________________\r\n        </td>\r\n        <td style=\"width:50%;\">\r\n            <b>Manager Approval:</b><br><br><br>\r\n            ____________________________\r\n        </td>\r\n    </tr>\r\n</table>\r\n\r\n<br><br>\r\n<hr>\r\n",
